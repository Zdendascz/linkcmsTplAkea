<script>
document.addEventListener('DOMContentLoaded', function() {
    // Definice fragmentu kódu, který chceme vložit
    var newMenuItemHtml = `<li class="akea-center-nav-menu-item">
                                <div class="akea-above-logo">
                                    <a href="index.html"><img src="https://doctetese.cz/templates/doctetese.cz/upload/logo-1-1.png" alt="" width="348" height="252" title="logo-1"></a>
                                </div>
